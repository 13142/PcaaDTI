                                                    {{else if (lookup (lookup ../../MemberType (math @index "-" 1)) "isBool")}}
                                                    <div class="anim-checkbox" style="flex: 0 0 auto;" draggable="false">
                                                        <label style="margin-top: 5px;">
                                                            <input type="checkbox" class="addEntryInput" value="">
                                                            <span class="cr">
                                                                <i class="cr-icon glyphicon glyphicon-ok"></i>
                                                            </span>
                                                        </label>
                                                    </div>
                                                    {{else}}
                                                    <input type="text" class="form-control addEntryInput" name="{{lookup (lookup ../../MemberType (math @index "-" 1)) "title"}}" id="{{lookup (lookup ../../MemberType (math @index "-" 1)) "title"}}" value="" placeholder="{{lookup (lookup ../../MemberType (math @index "-" 1)) "placeholder"}}" colID="{{@index}}" />
                                                    <!-- <span class="description">Max Length: <b class="maxL">{{lookup (lookup ../../MemberType (math @index "-" 1)) "maxSize"}}</b> Type: <b class="varType">{{../../MemberType.[@index].type}}</b> </span>-->
                                                    {{/if}}

                                                </div>
                                                {{/if}}
                                            </td>


                                            {{/each}}
                                            <td class="vert-align editBtnGroup">
                                                <div class="btn-group btn-group-sm pull-right show-on-hover">
                                                    <button type="button" class="btn btn-success editSaveBtn">Save</button>
                                                    <button type="button" class="btn btn-warning editCancelBtn">Cancel</button>
                                                    <button type="button" class="btn btn-default editEntryBtn">Edit</button>
                                                    <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#removalConfirm">Delete</button>
                                                </div>
                                            </td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                            <div>
                                <div class="pull-right" style="vertical-align: middle;"><button class="btn btn-primary" data-toggle="modal" data-target="#addMembers">Add</button></div>
                                <div class="massEditBtns">
                                    <label for="massEdit" style="vertical-align: middle;">Mass Edit:</label>
                                    <div class="btn-group btn-group-md">
                                        <button type="button" class="btn btn-default massEditEdit" data-toggle="modal" data-target="#addMembers">
                                            Edit
                                        </button>
                                        <button type="button" class="btn btn-danger massEditRemove" data-toggle="modal" data-target="#removalConfirm">
                                            Remove
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal fade addEntryModal" id="addMembers" tabindex="-1" role="dialog">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                        <h4 class="modal-title" id="">Add item</h4>
                                    </div>
                                    <div class="modal-body">
                                        Add entry to members:
                                        <div class="form-group">
                                            <div class="addEntryEntry">
                                                <span class="addEntryLbl" style="margin-right: 20px"><b>ID</b></span>
                                                <span class=""> Auto generated</span>
                                            </div>
